#include<stdio.h>
#include<stdlib.h>
struct node{

};

int main()
{
	void count_of nodes(struct node *head)
	{
	  int count= 0;
	  if(head == NULL)
	      printf("link list is empty");
	  struct node *ptr= NULL;
	  ptr= head;
	  while(ptr!=NULL)
	  {
	  	count++;
	  	ptr = ptr->link;
	  	
	  }
	  printf("%d",count);
}

	
}
